    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/sharp": "^0.31.1",
    "@types/sqlite3": "^3.1.8",
    "@types/uuid": "^9.0.2",
    "ts-node-dev": "^2.0.0",
